<!DOCTYPE html>
<html>
  <head>
    <title><%= yield(:title) %></title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <%= favicon_link_tag asset_path('favicon1.ico') %>
    <%= stylesheet_link_tag    'admin_items', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'admin_items', 'data-turbolinks-track': 'reload' %>
    <!-- Custom fonts for this template-->
    <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet"> 
  </head>
  <body>
    <%= yield %>
    <%= render 'layouts/footer' %>
  </body>
</html>
